<launch>
  <!-- Select the controller -->
  <arg name="controller" default="CustomControllers"/>

  <include file="$(find iiwa_driver)/launch/iiwa_bringup_ns.launch">
      <arg name="iiwa_ns" value="iiwa1" />
      <arg name="robot_name" value="iiwa"/>
      <arg name="model" value="14"/>
      <arg name="controller" value="$(arg controller)"/>
  </include>

</launch>